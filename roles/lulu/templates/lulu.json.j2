{
    "command": "/opt/lulu/systemd_exec.sh start",
    "config_files": [
        {
            "source": "{{ container_config_directory }}/config.prod.js",
            "dest": "/opt/lulu/config/config.prod.js",
            "owner": "root",
            "perm": "0600"
        }
    ],
    "permissions": [
        {
            "path": "/var/log/kolla/lulu",
            "owner": "root:root",
            "recurse": true
        }
    ]
}
